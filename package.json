{
  "name": "vaj-be",
  "version": "1.0.0",
  "description": "vintage archive jungle - backend project",
  "main": "index.js",
  "scripts": {
    "database-backup": "tsc -p tsconfig.scripts.json && node ./.temp/scripts/database-backup.js",
    "database-restore": "tsc -p tsconfig.scripts.json && node ./.temp/scripts/database-restore.js",
    "database-setup": "tsc -p tsconfig.scripts.json && node ./.temp/scripts/database-setup.js",
    "generate-transformer-classes": "tsc -p tsconfig.scripts.json && node ./.temp/scripts/generate-transformer-classes.js",
    "server": "nodemon --watch api --ext ts,tsx --exec ts-node ./src/api/server.ts",
    "generate-api-key": "tsc -p tsconfig.scripts.json && node ./.temp/scripts/generate-api-key.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "class-transformer": "^0.5.1",
    "express": "^5.1.0",
    "pg": "^8.15.6",
    "reflect-metadata": "^0.2.2",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.1",
    "@types/node": "^22.14.1",
    "@types/pg": "^8.11.14",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
